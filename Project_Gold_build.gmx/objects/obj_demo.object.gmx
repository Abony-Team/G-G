        <arguments>
          <argument>
            <kind>1</kind>
            <string>/*
** This object uses creatures a number of "particles" stored in an array
** I've chosen 100 as I needed a number that would lower FPS on my machine, So you may need to adjust this
**
*/
NUM_PARTICLES = 100;

for ( var i = 0 ; i &lt; NUM_PARTICLES ; i ++)
    {
    P[i,0] = choose(c_red,c_blue,c_green,c_purple) ; // Colour
    P[i,1] = irandom_range(8,24) ; //Radius
    P[i,2] = irandom_range(16,room_width-16) // X POS
    P[i,3] = irandom_range(16,room_height-16) // Y POS 
    P[i,4] = irandom_range(2,6) //Speed
    P[i,5] = irandom(359) // Direction
    }
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
