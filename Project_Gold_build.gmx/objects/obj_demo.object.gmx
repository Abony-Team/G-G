        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (keyboard_check(vk_space))
    {
    screen_save("SCREENSHOT.png") ;
    }

for ( var i = 0 ; i &lt; NUM_PARTICLES ; i ++)
    {
  
    //Collision Handling
    //It is this collision handling that causes slow-down
    //Note that this is really inefficient for the purpose of demostrating the Frame Skip
    for(var j = 0 ; j &lt; NUM_PARTICLES ; j++)
        {
        if (i != j)
            {
            if (point_distance(P[i,2],P[i,3],P[j,2],P[j,3]) &lt; P[i,1]+P[i,1])
                {
                P[i,5] = point_direction(P[i,2],P[i,3],P[j,2],P[j,3]) + 180 ;
                P[j,6] = P[i,5] - 180 ;
                }
            }
        }
        
    P[i,2] += lengthdir_x(P[i,4],P[i,5]) ;
    P[i,3] += lengthdir_y(P[i,4],P[i,5]) ;
    P[i,2] = P[i,2] mod room_width ;
    P[i,3] = P[i,3] mod room_height ;
    
    
    }
    
if (keyboard_check(vk_up))
    {
    

    
    P[NUM_PARTICLES,0] = choose(c_red,c_blue,c_green,c_purple) ; // Colour
    P[NUM_PARTICLES,1] = irandom_range(8,24) ; //Radius
    P[NUM_PARTICLES,2] = irandom_range(16,room_width-16) // X POS
    P[NUM_PARTICLES,3] = irandom_range(16,room_height-16) // Y POS 
    P[NUM_PARTICLES,4] = irandom_range(2,6) //Speed
    P[NUM_PARTICLES,5] = irandom(359) // Direction
    NUM_PARTICLES++ ;
    show_debug_message("NEW PARTICLE CREATED") ;
    }
    
if (keyboard_check(vk_down))
    {
    NUM_PARTICLES-- ;
    }
</string>
          </argument>
        </arguments>
      </action>
    </event>
