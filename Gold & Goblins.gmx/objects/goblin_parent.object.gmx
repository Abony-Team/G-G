            <string>//stats
obj_type = "enemy";
g_hp = global.g_hp;
speed = global.g_speed;
min_atk = global.g_minatk;
max_atk = global.g_maxatk;
a = 0;
click = true;
nsf = true;
atk = true;
x_finish = 0;
y_finish = 0;
vis = true;

//myPapa
if collision_circle(x,y,64,r_lair_obj,true,false) {
    myPapa = "lair";
    spawn = false;
}
else {
    myPapa = "control";
}

//for lair
if myPapa = "lair" {
    my_lair = collision_circle(x,y,32,r_lair_obj,true,false);
    x_plus = round(random(1));
    y_plus = round(random(1));

        if x_plus = 0 {
            x_finish = my_lair.x + 49 + round(random(32));
        }
        else {
            x_finish = my_lair.x - 49 - round(random(32));
        }

        if y_plus = 0 {
            y_finish = my_lair.y + 49 + round(random(32));
        }
        else {
            y_finish = my_lair.y - 49 - round(random(32));
        }

    move_towards_point(x_finish,y_finish,speed);
    direction = point_direction(x,y,x_finish,y_finish);
}
else { //for control
    //target
    randomize();
    var z;
    z = round(random(global.a));
    myTarget = global.target_array[z];
    move_towards_point(myTarget.x,myTarget.y,speed);
    direction = point_direction(x,y,myTarget.x,myTarget.y);
    
    if !place_empty(x,y) {
    var d,step;
    step = 7;
    for (d = 0; d &gt;= 0; d += 1) {
        if !place_empty(x,y) {    
            randomize();
            XX = round(random(1));
            YY = round(random(1));
        
                if XX = 0 {
                    
                    if YY = 0 {
                        x += step;
                        y += step;
                    }
                    else {
                        x += step;
                        y -= step;
                    }
                }
                else {
                    
                    if YY = 0 {
                        x -= step;
                        y += step;
                    }
                    else {
                        x -= step;
                        y -= step;
                    }
                }
        }
        else {
            break;
        }
    }
}
}
</string>
